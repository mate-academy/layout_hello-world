{
  "rules": {
    "line-end-style": false,
    "attr-no-dup": true,
    "attr-quote-style": false,
    "attr-req-value": true,
    "class-no-dup": true,
    "doctype-first": true,
    "doctype-html5": true,
    "fig-req-figcaption": true,
    "head-req-title": true,
    "html-req-lang": true,
    "id-class-style": false,
    "id-no-dup": true,
    "img-req-src": true,
    "img-req-alt": true,
    "input-radio-req-name": true,
    "spec-char-escape": true,
    "tag-name-lowercase": true,
    "tag-name-match": true,
    "tag-self-close": false,
